<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Avatar url:</strong>
    <%= user.avatar_url %>
  </p>

  <p>
    <strong>Contact info:</strong>
    <%= user.contact_info %>
  </p>

  <p>
    <strong>Inspirational quote:</strong>
    <%= user.inspirational_quote %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= user.bio %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

</div>
